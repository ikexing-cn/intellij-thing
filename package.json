{
  "name": "intellij-thing",
  "displayName": "IntelliJ Thing",
  "version": "0.0.0",
  "publisher": "ikexing",
  "packageManager": "pnpm@7.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/antfu/intellij-thing#readme",
  "bugs": {
    "url": "https://github.com/antfu/intellij-thing/issues"
  },
  "license": "MIT",
  "author": "ikexing <ikexing@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikexing/intellij-thing"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "res/icon.png",
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.41",
    "@types/vscode": "^1.68.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "pnpm": "^7.1.9",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.11",
    "vitest": "^0.14.1",
    "vsce": "^2.9.1"
  }
}
